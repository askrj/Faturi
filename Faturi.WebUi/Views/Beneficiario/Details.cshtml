@model Faturi.Application.DTOs.BeneficiarioDTO

<h3>Product Details</h3>
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">@Html.DisplayNameFor(model => model.Nome)</dt>
        <dd class="col-sm-10">@Html.DisplayFor(model => model.Nome)</dd>
        <dt class="col-sm-2">@Html.DisplayNameFor(model => model.CPF)</dt>
        <dd class="col-sm-10">@Html.DisplayFor(model => model.CPF)</dd>
        <dt class="col-sm-2">@Html.DisplayNameFor(model => model.Foto)</dt>
        <dd class="col-sm-10">
            @if (!ViewBag.ImageExist)
            {
                <img src="~/images/noimage.png" class="rounded-circle"
                 height="300" width="300" asp-append-version="true" />
            }
            else
            {
                <img src="~/images/@Model.Foto" class="rounded-circle"
                 height="300" width="300" asp-append-version="true" />
            }
        </dd>
        <dt class="col-sm-2">@Html.DisplayNameFor(model => model.Carteira)</dt>
        <dd class="col-sm-10">@Html.DisplayFor(model => model.Carteira)</dd>
        <dt class="col-sm-2">@Html.DisplayNameFor(model => model.Nascimento)</dt>
        <dd class="col-sm-10">@Html.DisplayFor(model => model.Nascimento)</dd>
        <dt class="col-sm-2">@Html.DisplayNameFor(model => model.Convenio)</dt>
        <dd class="col-sm-10">@Html.DisplayFor(model => model.Convenio.Nome)</dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
