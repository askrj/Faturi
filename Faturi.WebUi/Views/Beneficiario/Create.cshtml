@model Faturi.Application.DTOs.BeneficiarioDTO

<h2>Register new Product</h2>

<form asp-action="Create">

    <div class="form-horizontal">

        <hr />

        <div asp-validation-summary="All" class="text-danger"></div>

        <div class="form-group">
            <label asp-for="Nome" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="Nome" class="form-control" />
                <span asp-validation-for="Nome" class="text-danger"></span>
            </div>
        </div>

        <div class="form-group">
            <label asp-for="Carteira" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="Carteira" class="form-control" />
                <span asp-validation-for="Carteira" class="text-danger"></span>
            </div>
        </div>

        <div class="form-group">
            <label asp-for="Foto" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="Foto" class="form-control" />
                <span asp-validation-for="Foto" class="text-danger"></span>
            </div>
        </div>

        <div class="form-group">
            <label asp-for="CPF" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="CPF" class="form-control" />
                <span asp-validation-for="CPF" class="text-danger"></span>
            </div>
        </div>

        <div class="form-group">
            <label asp-for="Nascimento" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="Nascimento" class="form-control" />
                <span asp-validation-for="Nascimento" class="text-danger"></span>
            </div>
        </div>


        <div class="form-group">
            <label asp-for="ConvenioId" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <select asp-for="ConvenioId" class="form-control" asp-items="ViewBag.ConvenioId"> </select>
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-success" />
                <a asp-action="Index" class="btn btn-info">Back to List</a>
            </div>
        </div>
    </div>
</form>
